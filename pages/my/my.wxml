<view class="meContainer">
    <view class="header">
        <view class="avatar">
            <open-data type='userAvatarUrl'></open-data>
        </view>
        <view class="details">
            <open-data type='userNickName' class="name"></open-data>
            <view class="account">余额: {{account}}</view>
        </view>
        <button open-type="contact" class="qr">问题反馈</button>
    </view>
    <view>{{isLogin}}</view>
</view>

